generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model AuthCreds {
  id        Int    @id @default(autoincrement())
  sessionId String @unique
  creds String @db.Text

  @@map("auth_creds")
}

model SignalKey {
  id      String  @id
  type    String  @db.VarChar(255)
  data    String @db.Text

  @@map("signal_key")
}